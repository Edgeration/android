<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="edgeration"
           fetch="https://github.com/edgeration"
           revision="refs/heads/edgeration-0.1" />

  <!-- AOSP Projects -->
  <project path="build/make" name="build" groups="pdk" remote="edgeration" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="build_soong" groups="pdk,tradefed" remote="edgeration" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <project path="android" name="android" remote="edgeration" />

  <!-- SEPolicy -->
  <project path="device/lineage/sepolicy" remote="edgeration" name="device_lineage_sepolicy" />

  <!-- Frameworks -->
  <project path="frameworks/base" name="frameworks_base" remote="edgeration" />

  <!-- Packages -->
  <project path="packages/apps/Settings" name="packages_apps_Settings" remote="edgeration" />
  <project path="packages/apps/Updates" name="packages_apps_Updates" remote="edgeration" />
  <project path="packages/apps/LineageParts" name="packages_apps_LineageParts" remote="edgeration" />
  <project path="packages/apps/EdgerationSDK" name="packages_apps_EdgerationSDK" remote="edgeration"/>

  <!-- System -->
  <project path="vendor/edgeration" name="vendor_edgeration" remote="edgeration" />
  <project path="lineage-sdk" name="lineage-sdk" remote="edgeration" />
  <project path="system/core" name="system_core" remote="edgeration" />

</manifest>
